{
  "name": "q-economics",
  "version": "1.0.0",
  "description": "Online testing portal for students",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "backup": "node server/scripts/backup.js",
    "backup-status": "node server/scripts/backup-status.js",
    "init-backup": "mkdir -p backups && node server/scripts/backup.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "build": "npm run install-client && npm run build-client",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "ci": "npm run lint && npm run test",
    "logs:rotate": "node server/scripts/rotate-logs.js",
    "logs:clean": "node server/scripts/clean-logs.js",
    "perf:load": "node server/tests/performance/loadTest.js",
    "perf:stress": "node server/tests/performance/stressTest.js",
    "perf:all": "npm run perf:load && npm run perf:stress"
  },
  "dependencies": {
    "@slack/webhook": "^6.1.0",
    "aws-sdk": "^2.1499.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-elasticsearch": "^0.17.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/jest": "^27.5.2",
    "autocannon": "^7.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^27.5.1",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
